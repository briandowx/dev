        <errorConditionFormula>LEN(Zip_Code__c) &gt; 0 &amp;&amp; (Country__c = &quot;United States&quot; || Country__c = &quot;US&quot;) &amp;&amp; VLOOKUP( $ObjectType.ZIP_Code__c.Fields.City__c,  $ObjectType.ZIP_Code__c.Fields.Name, LEFT(Zip_Code__c, 5)) &lt;&gt; City__c</errorConditionFormula>
        <errorDisplayField>Zip_Code__c</errorDisplayField>
        <errorMessage>The ZIP Code you entered is incorrect</errorMessage>
    </validationRules>
